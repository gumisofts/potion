{
    "dev": {
        "aws_region": "eu-north-1",
        "django_settings": "core.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "potion",
        "project_name": "potion",
        "runtime": "python3.12",
        "s3_bucket": "potion-v01",
        "slim_handler": true,
        "certificate_arn": "arn:aws:acm:us-east-1:626635425063:certificate/dae101ec-26dc-4170-9d2d-20858cc710b1",
        "domain": "potion.dev.gumisofts.com",
        "route53_enabled": false,
        "events": [
            {
                "function": "tasks_handler.tasks.subscriptions_payment",
                "expression": "rate(1 day)"
            }
        ],
        "extra_permissions": [
            {
                "Effect": "Allow",
                "Action": [
                    "s3:GetObject",
                    "s3:PutObject",
                    "s3:ListBucket"
                ],
                "Resource": [
                    "arn:aws:s3:::potion-dev-v01",
                    "arn:aws:s3:::potion-dev-v01/*"
                ]
            }
        ]
    },
    "pro": {
        "aws_region": "eu-north-1",
        "django_settings": "core.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "potion",
        "project_name": "potion",
        "runtime": "python3.12",
        "s3_bucket": "potion-v01",
        "slim_handler": true,
        "certificate_arn": "arn:aws:acm:us-east-1:626635425063:certificate/dae101ec-26dc-4170-9d2d-20858cc710b1",
        "domain": "potion.v01.gumisofts.com"
    }
}